<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/mubiaomingcheng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="目标名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/mubiaomingcheng_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="目标名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="制定时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/zhidingshijian_tv"
            android:text="请选择制定时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/duanlianmubiao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="锻炼目标"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/duanlianmubiao_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="锻炼目标"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yundongmubiao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="运动目标"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yundongmubiao_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="运动目标"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yinshimubiao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="饮食目标"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yinshimubiao_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="饮食目标"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="计划开始"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/jihuakaishi_tv"
            android:text="请选择计划开始"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="计划结束"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/jihuajieshu_tv"
            android:text="请选择计划结束"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/zhanghao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="账号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/zhanghao_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="账号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/xingming_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="姓名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/xingming_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="姓名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFrameLayout
            style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>

    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
