<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/mubiaomingcheng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="目标名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/mubiaomingcheng_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="目标名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="运动类型"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/yundongleixing_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择运动类型"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="开始时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/kaishishijian_tv"
            android:text="请选择开始时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="结束时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/jieshushijian_tv"
            android:text="请选择结束时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yundongshizhang_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="运动时长(m)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yundongshizhang_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="运动时长(m)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/xiaohaokaluli_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="消耗卡路里"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/xiaohaokaluli_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="消耗卡路里"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="记录时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/jilushijian_tv"
            android:text="请选择记录时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/zhanghao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="账号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/zhanghao_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="账号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/xingming_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="姓名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/xingming_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="姓名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFrameLayout
            style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>



    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
