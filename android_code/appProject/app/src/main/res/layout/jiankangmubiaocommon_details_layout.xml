<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/srv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
        <androidx.core.widget.NestedScrollView
            android:fillViewport="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
                <com.design.appproject.widget.MyLinearLayout
                    style="@style/detail_box_style"
                    android:orientation="vertical">
                    <com.design.appproject.widget.MyFlexBoxLayout
                        app:flexWrap="wrap"
                        style="@style/detail_detail_box_style">
                       <com.design.appproject.widget.MyFlexBoxLayout
                           app:flexWrap="wrap"
                           style="@style/detail_detail_item_box_style">
                           <com.design.appproject.widget.MyTextView
                               android:text="目标名称："
                               style="@style/detail_detail_item_label_style"/>
                           <com.design.appproject.widget.MyTextView
                                android:id="@+id/mubiaomingcheng_tv"
                               tools:text="目标名称："
                               style="@style/detail_detail_item_text_style"/>
                       </com.design.appproject.widget.MyFlexBoxLayout>
                       <com.design.appproject.widget.MyFlexBoxLayout
                           app:flexWrap="wrap"
                           style="@style/detail_detail_item_box_style">
                           <com.design.appproject.widget.MyTextView
                               android:text="制定时间："
                               style="@style/detail_detail_item_label_style"/>
                           <com.design.appproject.widget.MyTextView
                                android:id="@+id/zhidingshijian_tv"
                               tools:text="制定时间："
                               style="@style/detail_detail_item_text_style"/>
                       </com.design.appproject.widget.MyFlexBoxLayout>
                       <com.design.appproject.widget.MyFlexBoxLayout
                           app:flexWrap="wrap"
                           style="@style/detail_detail_item_box_style">
                           <com.design.appproject.widget.MyTextView
                               android:text="姓名："
                               style="@style/detail_detail_item_label_style"/>
                           <com.design.appproject.widget.MyTextView
                                android:id="@+id/xingming_tv"
                               tools:text="姓名："
                               style="@style/detail_detail_item_text_style"/>
                       </com.design.appproject.widget.MyFlexBoxLayout>
                        <com.design.appproject.widget.MyFlexBoxLayout
                            style="@style/detail_detail_item_box_style">
                            <com.design.appproject.widget.MyTextView
                                android:text="锻炼目标："
                                style="@style/detail_detail_item_label_style"/>
                            <com.design.appproject.widget.MyTextView
                                android:id="@+id/duanlianmubiao_tv"
                                style="@style/detail_detail_item_text_style"/>
                        </com.design.appproject.widget.MyFlexBoxLayout>
                        <com.design.appproject.widget.MyFlexBoxLayout
                            style="@style/detail_detail_item_box_style">
                            <com.design.appproject.widget.MyTextView
                                android:text="运动目标："
                                style="@style/detail_detail_item_label_style"/>
                            <com.design.appproject.widget.MyTextView
                                android:id="@+id/yundongmubiao_tv"
                                style="@style/detail_detail_item_text_style"/>
                        </com.design.appproject.widget.MyFlexBoxLayout>
                        <com.design.appproject.widget.MyFlexBoxLayout
                            style="@style/detail_detail_item_box_style">
                            <com.design.appproject.widget.MyTextView
                                android:text="饮食目标："
                                style="@style/detail_detail_item_label_style"/>
                            <com.design.appproject.widget.MyTextView
                                android:id="@+id/yinshimubiao_tv"
                                style="@style/detail_detail_item_text_style"/>
                        </com.design.appproject.widget.MyFlexBoxLayout>
                        <com.design.appproject.widget.MyFlexBoxLayout
                            style="@style/detail_detail_item_box_style">
                            <com.design.appproject.widget.MyTextView
                                android:text="计划开始："
                                style="@style/detail_detail_item_label_style"/>
                            <com.design.appproject.widget.MyTextView
                                android:id="@+id/jihuakaishi_tv"
                                style="@style/detail_detail_item_text_style"/>
                        </com.design.appproject.widget.MyFlexBoxLayout>
                        <com.design.appproject.widget.MyFlexBoxLayout
                            style="@style/detail_detail_item_box_style">
                            <com.design.appproject.widget.MyTextView
                                android:text="计划结束："
                                style="@style/detail_detail_item_label_style"/>
                            <com.design.appproject.widget.MyTextView
                                android:id="@+id/jihuajieshu_tv"
                                style="@style/detail_detail_item_text_style"/>
                        </com.design.appproject.widget.MyFlexBoxLayout>
                        <com.design.appproject.widget.MyFlexBoxLayout
                            style="@style/detail_detail_item_box_style">
                            <com.design.appproject.widget.MyTextView
                                android:text="账号："
                                style="@style/detail_detail_item_label_style"/>
                            <com.design.appproject.widget.MyTextView
                                android:id="@+id/zhanghao_tv"
                                style="@style/detail_detail_item_text_style"/>
                        </com.design.appproject.widget.MyFlexBoxLayout>
                    </com.design.appproject.widget.MyFlexBoxLayout>
                    <com.design.appproject.widget.MyFlexBoxLayout
                        style="@style/detail_detail_btn_box_style">
                        <com.design.appproject.widget.MyButton
                            android:gravity="center"
                            android:id="@+id/crossOptButton_btn0"
                            android:text="开始运动"
                            style="@style/detail_detail_btn_common_style"/>
                        <com.design.appproject.widget.MyButton
                            android:gravity="center"
                            android:id="@+id/crossOptButton_btn1"
                            android:text="发送建议"
                            style="@style/detail_detail_btn_common_style"/>
                    </com.design.appproject.widget.MyFlexBoxLayout>
                </com.design.appproject.widget.MyLinearLayout>
        </androidx.core.widget.NestedScrollView>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

